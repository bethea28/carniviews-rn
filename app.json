{
  "expo": {
    "name": "RN-STARTER",
    "slug": "RN-STARTER",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library to select photos."
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.bethea28.rn-starter",
      "config": {
        "googleSignIn": {
          "reservedClientId": "743443225844-tg0083sojer7e57dlj91q1eg3s4qqnhn.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ],
      "package": "com.bethea28.rnstarter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["@react-native-google-signin/google-signin"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "apiKey": "AIzaSyDb7uPi0x8ZIglrw-1-m30dX8JFHeHzZak",
          "authDomain": "carniviews.firebaseapp.com",
          "projectId": "carniviews",
          "storageBucket": "carniviews.firebasestorage.app",
          "messagingSenderId": "389796448834",
          "appId": "1:389796448834:android:6b01b6090e56e14220bbdb",
          "measurementId": "YOUR_MEASUREMENT_ID"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8a06620d-d7f6-4df7-b25d-3d2364fe2361"
      }
    }
  }
}
